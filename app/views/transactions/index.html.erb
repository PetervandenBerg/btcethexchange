<table class="table table-striped">
  <tr>
    <th>Created at</th>
    <th>Address</th>
    <th>Credit</th>
    <th>Debit</th>
    <th>txid</th>

  </tr>
    <% @transactions.each do |t|  %>
      <tr>
        <td><%= t.created_at %></td>
        <td><%= t.address %></td>
        <td><%= t.credit %></td>
        <td>- <%= t.debit %></td>
        <td><%= t.transaction_hash %></td>
      </tr>
    <% end %>   
</table>



